{
	"name": "pyro-client-extension",
	"displayName": "Pyro Client",
	"description": "A client for a pyro generated application.",
	"version": "0.0.1",
	"publisher": "ls5",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "scce.pyro",
				"displayName": "Pyro Editor",
				"selector": [
					{
						"filenamePattern": "*.cinco"
					}
				],
				"priority": "option"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"package": "vsce package",
		"prepare": "yarn run installExtensionYarn",
		"installExtensionNPM": "npm run package",
		"installExtensionYarn": "yarn run package",
		"deleteTmp": "rimraf pyro-client-0.0.1.vsix",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.3.2"
	}
}
