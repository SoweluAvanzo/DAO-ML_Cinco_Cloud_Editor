stylePath "codec.msl"

@FileCodec("ExampleFileCodecHandler")
@Hook("ExampleFileCodecHook", OnOpen)
graphModel CodecTest {
    diagramExtension "codec"
    
    containableElements(SomeNode)
}  

node SomeNode {
    style labeledCircle("${label}")
    
    incomingEdges (Transition)
    outgoingEdges (Transition)
    attr string as label
}	
    
edge Transition {
    style simpleArrow
}
