// direct html, css and javascript code
nodeStyle webviewExampleCode {
	webView {
        size (100, 100)
        scrollable false
        content ("<h1 style='color: #FF77FF'>Hello World</h1><script>console.log('hello world!')</script>")
	}
}

// referenced file inside the language-folder
nodeStyle webviewExampleRef {
	webView {
        size (400, 340)
        content ("webview.html") 
	}
}

// url that will be displayed
nodeStyle webviewExampleLink {
	webView {
        size (500, 500)
        padding 10
        content ("https://cinco.cloud")
	}
}

// param that will be injected to content
nodeStyle webviewExampleParam(1) {
	webView {
        scrollable true
        content ("%s")
	}
}
