{
  "name": "@cinco-glsp/cinco-glsp-server",
  "version": "1.0.0",
  "description": "GLSP node server for Cinco GSLP diagrams",
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "keywords": [
    "eclipse",
    "graphics",
    "diagram",
    "modeling",
    "visualization",
    "glsp",
    "diagram editor"
  ],
  "author": {
    "name": "Eclipse GLSP"
  },
  "homepage": "https://www.eclipse.org/glsp/",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-glsp/glsp-examples.git"
  },
  "bugs": "https://github.com/eclipse-glsp/glsp/issues",
  "contributors": [
    {
      "name": "Eclipse GLSP Project",
      "email": "glsp-dev@eclipse.org",
      "url": "https://projects.eclipse.org/projects/ecd.glsp"
    }
  ],
  "files": [
    "lib",
    "src",
    "languages"
  ],
  "dependencies": {
    "@eclipse-glsp/server-node": "1.0.0",
    "@theia/core": "^1.34.1"
  },
  "devDependencies": {
    "@eclipse-glsp/config": "^1.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "fs-extra": "^10.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "4.5.2",
    "webpack": "^5.48.0",
    "webpack-cli": "4.7.0",
    "ignore-loader": "^0.1.2"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "install:only": "yarn install --ignore-scripts",
    "clean": "rimraf tsconfig.tsbuildinfo lib bundle && yarn node clean-languages.js",
    "build": "yarn install:only && tsc && yarn bundle",
    "bundle": "webpack",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "watch": "tsc -w",
    "start": "node --enable-source-maps lib/src/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/src/index",
  "types": "lib/src/index"
}
