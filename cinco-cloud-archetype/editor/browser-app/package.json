{
  "private": true,
  "name": "browser-app",
  "version": "1.0.0",
  "dependencies": {
    "electron": "~25.3",
    "@theia/core": "~1.38",
    "@theia/callhierarchy": "~1.38",
    "@theia/console": "~1.38",
    "@theia/debug": "~1.38",
    "@theia/editor": "~1.38",
    "@theia/electron": "~1.38",
    "@theia/file-search": "~1.38",
    "@theia/filesystem": "~1.38",
    "@theia/keymaps": "~1.38",
    "@theia/markers": "~1.38",
    "@theia/messages": "~1.38",
    "@theia/metrics": "~1.38",
    "@theia/mini-browser": "~1.38",
    "@theia/monaco": "~1.38",
    "@theia/navigator": "~1.38",
    "@theia/outline-view": "~1.38",
    "@theia/output": "~1.38",
    "@theia/plugin-dev": "~1.38",
    "@theia/plugin-ext": "~1.38",
    "@theia/plugin-ext-vscode": "~1.38",
    "@theia/plugin-metrics": "~1.38",
    "@theia/preferences": "~1.38",
    "@theia/process": "~1.38",
    "@theia/property-view": "~1.38",
    "@theia/search-in-workspace": "~1.38",
    "@theia/task": "~1.38",
    "@theia/terminal": "~1.38",
    "@theia/typehierarchy": "~1.38",
    "@theia/userstorage": "~1.38",
    "@theia/variable-resolver": "~1.38",
    "@theia/vsx-registry": "~1.38",
    "@theia/workspace": "~1.38",
    "cinco-authenticator": "1.0.0",
    "cinco-branding": "1.0.0",
    "@cinco-glsp/cinco-theia": "1.0.0"
  },
  "devDependencies": {
    "@theia/cli": "~1.38"
  },
  "scripts": {
    "prepare": "yarn run download:plugins && theia build",
    "theia": "theia",
    "start": "theia start --port=3000 --CINCO_GLSP=5007 --root-dir=../workspace --plugins=local-dir:./plugins",
    "debug": "theia start --port=3000 --CINCO_GLSP=5007 --remote-debugging-port=9222 --loglevel=debug --hostname=localhost --no-cluster --root-dir=../workspace --loglevel=debug --plugins=local-dir:./plugins --app-project-path=../",
    "watch": "theia build --watch --mode development",
    "download:plugins": "theia download:plugins"
  },
  "theia": {
    "target": "browser"
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
  }
}
