{
  "private": true,
  "name": "browser-app",
  "version": "1.0.0",
  "dependencies": {
    "@theia/core": "1.28.0",
    "@theia/callhierarchy": "1.28.0",
    "@theia/console": "1.28.0",
    "@theia/debug": "1.28.0",
    "@theia/editor": "1.28.0",
    "@theia/file-search": "1.28.0",
    "@theia/filesystem": "1.28.0",
    "@theia/keymaps": "1.28.0",
    "@theia/markers": "1.28.0",
    "@theia/messages": "1.28.0",
    "@theia/metrics": "1.28.0",
    "@theia/mini-browser": "1.28.0",
    "@theia/monaco": "1.28.0",
    "@theia/navigator": "1.28.0",
    "@theia/outline-view": "1.28.0",
    "@theia/output": "1.28.0",
    "@theia/plugin-dev": "1.28.0",
    "@theia/plugin-ext": "1.28.0",
    "@theia/plugin-ext-vscode": "1.28.0",
    "@theia/plugin-metrics": "1.28.0",
    "@theia/preferences": "1.28.0",
    "@theia/process": "1.28.0",
    "@theia/property-view": "1.28.0",
    "@theia/search-in-workspace": "1.28.0",
    "@theia/task": "1.28.0",
    "@theia/terminal": "1.28.0",
    "@theia/typehierarchy": "1.28.0",
    "@theia/userstorage": "1.28.0",
    "@theia/variable-resolver": "1.28.0",
    "@theia/vsx-registry": "1.28.0",
    "@theia/workspace": "1.28.0",
    "cinco-authenticator": "1.0.0",
    "cinco-branding": "1.0.0"
  },
  "devDependencies": {
    "@theia/cli": "1.28.0"
  },
  "scripts": {
    "prepare": "yarn run download:plugins && theia build --mode development",
    "theia": "theia",
    "start": "theia start --port=3000 --root-dir=../workspace --plugins=local-dir:./plugins",
    "debug": "theia start --port=3000 --remote-debugging-port=9222 --loglevel=debug --hostname=localhost --no-cluster --root-dir=../workspace --loglevel=debug --plugins=local-dir:./plugins --app-project-path=../",
    "watch": "theia build --watch --mode development",
    "download:plugins": "theia download:plugins"
  },
  "theia": {
    "target": "browser"
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-builtin-java": "https://github.com/theia-ide/vscode-builtin-extensions/releases/download/v1.39.1-prel/java-1.39.1-prel.vsix"
  }
}
