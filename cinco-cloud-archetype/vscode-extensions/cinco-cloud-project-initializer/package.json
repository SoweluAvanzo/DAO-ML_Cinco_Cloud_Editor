{
  "name": "cinco-cloud-project-initializer",
  "displayName": "Cinco Cloud Project Initializer",
  "description": "",
  "version": "0.0.1",
  "author": "ls5",
  "license": "EPL-2.0",
  "repository": "https://gitlab.com/scce/cinco-cloud",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cincoCloud.initializeProject",
        "title": "Initialize Project / Create Example",
        "category": "Cinco Cloud"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "postinstall": "vsce package",
    "vsce": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "rimraf": "^5.0.5",
    "simple-git": "^3.20.0",
    "vsce": "^2.15.0"
  }
}
