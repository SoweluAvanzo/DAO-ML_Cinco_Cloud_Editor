{
	"name": "cinco-project-initializer",
	"displayName": "Cinco Project Initializer",
	"description": "This extension registeres commands to create a predefined example project into the workspace.",
	"author": "ls5",
	"license": "MIT",
	"version": "0.0.1",
	"publisher": "ls5",
	"repository": "https://gitlab.com/scce/cinco-cloud-archetype",
	"categories": [
		"Other"
	],
	"keywords": [
		"Cinco",
		"MGL",
		"CPD",
		"Style"
	],
	"engines": {
		"vscode": "^1.54.0"
	},
	"activationEvents": [
		"*",
		"onCommand:info.scce.cinco-cloud.initialize-project"
	],
	"main": "./out/extension/bundle",
	"contributes": {
		"commands": [
			{
				"command": "info.scce.cinco-cloud.initialize-project",
				"title": "Create Example",
				"category": "Cinco Cloud"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"postinstall": "npm run compile && vsce package",
        "build": "tsc --build",
        "bundle-extension": "esbuild out/extension/main.js --bundle --platform=node --external:vscode --format=cjs --outfile=out/extension/bundle.js",
        "bundle-webview": "esbuild out/webview/main.js --bundle --outfile=out/webview/bundle.js",
		"compile": "npm run build && npm run bundle-extension && npm run bundle-webview"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/mocha": "^8.0.3",
		"@types/node": "14.14.16",
		"@types/vscode": "1.49.0",
		"@typescript-eslint/parser": "^2.3.0",
		"esbuild": "^0.14.29",
		"eslint": "^6.4.0",
		"mocha": "^8.1.1",
		"run-script-os": "1.1.1",
		"typescript": "^4.2.2",
		"vsce": "1.85.1"
	},
	"dependencies": {
		"adm-zip": "0.4.16",
		"copyfiles": "2.3.0",
		"fs-extra": "^10.0.1",
		"graphviz": "0.0.9",
		"gunzip-file": "0.1.1",
		"lru-cache": "6.0.0",
		"minimatch": "3.0.4",
		"original-fs": "1.1.0",
		"request": "2.88.2",
		"request-progress": "3.0.0",
		"rimraf": "3.0.2",
		"ssp-dos2unix-js": "1.0.1",
		"tar-fs": "2.1.0",
		"unzipper": "0.10.11",
		"vscode-jsonrpc": "6.0.0",
		"vscode-languageclient": "^7.0.0",
		"vscode-languageserver-protocol": "3.15.3",
		"which": "2.0.2"
	},
	"config": {
		"unsafe-perm": true
	}
}
