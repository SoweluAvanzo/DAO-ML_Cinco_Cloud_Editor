@pyroIncludeSources( // source folders
	"info.scce.cinco.product.extendedflowgraph/src",
	"info.scce.cinco.product.flowgraph/src",
	"info.scce.cinco.product.webstory/src",
	"info.scce.cinco.product.base.process/src",
	"info.scce.cinco.product.ml.process/src",
	"info.scce.rig/src"
)
@pyroProjectService(
	"info.scce.cinco.product.ml.process.JupyterService", // Implementation
	"Connect Jupyter Account", // Name
	"Username",	// arg0
	"URL",		// arg1
	"Token"		// arg2
)
// a package of graphElements, which are not directly creatable
@pyroTransientAPI("model/BaseProcess.mgl") 
cincoProduct Test {
	mgl "model/Empty.mgl"
	mgl "model/Hierarchy.mgl"
	mgl "model/HooksAndActions.mgl"		
	mgl "model/Attributes.mgl"						
	mgl "model/PrimeRefs.mgl"			
	mgl "model/ExtendedFlowGraph.mgl"
	mgl "model/FlowGraph.mgl"
	mgl "model/WebStory.mgl"
	mgl "model/MLProcess.mgl"
	mgl "model/Pipeline.mgl"

	splashScreen "branding/splash.bmp" { // TODO: deprecated? currently using branding
		progressBar (37,268,190,10)
		progressMessage (37,280,190,18)
	}

	// TODO: favicon branding only in loading-screen
	image16 "branding/Icon16.png"
	image32 "branding/Icon32.png"
	image48 "branding/Icon48.png"
	image64 "branding/Icon64.png"
	image128 "branding/Icon128.png"

	// TODO: not used at all
	about {
		text "This is the test-project for the Cinco-Cloud"
	}
}